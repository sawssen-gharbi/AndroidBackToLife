<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:fontFamily">@font/avenir</item>
    </style>
    <style name="expandedBarTextStyle" parent="AppTheme">
        <!-- Customize your theme here. -->
        <item name="android:textSize">0sp</item>
        <item name="android:textColor">@android:color/transparent</item>
        <item name="android:fontFamily">@font/avenir</item>

    </style>
    <style name="appCompatButtonStyle" parent="Widget.AppCompat.Button.Colored">
        <item name="backgroundTint">@color/back</item>
        <item name="android:textColor">@color/beige</item>
        <item name="android:fontFamily">@font/avenir</item>
    </style>

</resources>